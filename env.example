# API Keys
GEMINI_API_KEY=your_gemini_api_key_here

# Pinecone Vector Database
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment_here

# Cloudflare Workers
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id_here
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here

# Vercel
VERCEL_TOKEN=your_vercel_token_here
VERCEL_PROJECT_ID=your_vercel_project_id_here

# API Configuration
API_BASE_URL=https://your-api-domain.workers.dev
API_BASE_URL_DEV=http://localhost:8787

# RAG Configuration
RAG_MANIFEST_URL=https://your-domain.com/rag/manifest.json
RAG_MANIFEST_URL_DEV=http://localhost:8787/rag/manifest.json
RAG_DOCS_URL=https://your-domain.com/rag/docs.jsonl
RAG_DOCS_URL_DEV=http://localhost:3000/rag/docs.jsonl

# Redis (for caching)
REDIS_URL=your_redis_url_here

# CORS Origins
CORS_ORIGINS=http://localhost:3000,https://your-domain.com

# Feature Flags
ENABLE_RAG=true
ENABLE_VECTOR_SEARCH=true
ENABLE_CACHE=true

# Logging
LOG_LEVEL=info
NODE_ENV=development
