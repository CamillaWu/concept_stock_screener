{
  "name": "development",
  "description": "開發環境配置",
  "version": "1.0.0",
  "environment": "development",
  "api": {
    "baseUrl": "http://localhost:8787",
    "timeout": 30000,
    "retries": 3
  },
  "web": {
    "baseUrl": "http://localhost:3000",
    "port": 3000
  },
  "dataPipeline": {
    "baseUrl": "http://localhost:8000",
    "port": 8000
  },
  "database": {
    "type": "sqlite",
    "path": "./data/dev.db"
  },
  "cache": {
    "type": "memory",
    "ttl": 3600
  },
  "logging": {
    "level": "debug",
    "format": "json"
  },
  "features": {
    "debug": true,
    "hotReload": true,
    "mockData": true
  }
}
